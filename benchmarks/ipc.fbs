namespace ipc;

union Payload {
  AddRequest,
  AddResponse,
  MyRandRequest,
  MyRandResponse
}

table AddRequest {
  x: double;
  y: double;
}

table AddResponse {
  result: double;
}

table MyRandRequest {
}

table MyRandResponse {
  result: double;
}

table Message {
  req_id: ulong;
  payload: Payload;
}

root_type Message;
